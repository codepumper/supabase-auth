{
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "jsxSingleQuote": false,
    "jsxBracketSameLine": false,
    "arrowParens": "always",
    "endOfLine": "auto",
    "bracketSpacing": true,
    "quoteProps": "as-needed",
    "overrides": [
      {
        "files": "*.md",
        "options": {
          "printWidth": 100
        }
      }
    ]
  }
  
  
  
  